#include<iostream>
using namespace std;

class jj
{
public:                 void dm(){
    int grade;
    cout<<"enter grade: ";
    cin>>grade;
        if(grade>=90){
            cout<<"A";
        }
        else if(grade>=80){
            cout<<"B";
        }
        
        else if(grade>=70){
            cout<<"c";
        }
        
        else if(grade>=60){
            cout<<"D";
        }
        
        else if(grade>=0){
            cout<<"FAIL";
        }
        else{
            cout<<"error";
        }
    }
};

int main(){

    jj  myjj;
    myjj.dm();
}
